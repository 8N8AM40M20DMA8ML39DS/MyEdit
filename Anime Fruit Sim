if not game:IsLoaded() then
	game.Loaded:Wait()
end
local VirtualUser = game:GetService("VirtualUser")
local VirtualInputManager = game:GetService("VirtualInputManager")
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local function CreateWindow()
	return OrionLib:MakeWindow({Name = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name, HidePremium = true, SaveConfig = true, ConfigFolder = "AnimeFruit", IntroEnabled = false, IntroText = ""})
end
if game.PlaceId == 10446125875 then -- Anime Fruit Sim
    local Window = CreateWindow()
    local Main = Window:MakeTab({
		Name = "Main",
		Icon = "rbxassetid://4483345998",
		PremiumOnly = false
	})
	
Main:AddToggle({
		Name = "Auto Attack",
		Default = false,
		Save = true,
		Flag = "ATK",
		Callback = function(Value)
			MobHIT = Value
			while MobHIT and task.wait(.1) do
                game:GetService("ReplicatedStorage").Remotes.Attack:FireServer()
			end
	end
	})
	
	
Main:AddToggle({
		Name = "Auto Skill",
		Default = false,
		Save = true,
		Flag = "SKILL",
		Callback = function(Value)
			SKILL = Value
			while SKILL and task.wait(.1) do
                game:GetService("ReplicatedStorage").Remotes.Validate:FireServer("One")
                game:GetService("ReplicatedStorage").Remotes.Validate:FireServer("Two")
                game:GetService("ReplicatedStorage").Remotes.Validate:FireServer("Three")
		    end
	end
	})
	Main:AddDropdown({
		Name = "SelectedEggToOpen",
		Options = {"FruitEgg", "BasicEgg", "BizarreEgg", "DesertEgg", "JungleEgg","DominusEgg","DarkEgg","GlitchedEgg","IceEgg","OceanEgg","MagmaEgg","CrystalEgg","AncientEgg","VolcanoEgg","MetalicEgg"},
		Save = true,
		Flag = "SelectedEgg",
		Callback = function(Value)
			SelectedEgg = Value
		end
	})
Main:AddToggle({
		Name = "Open Egg",
		Default = false,
		Save = true,
		Flag = "OpenLooping",
		Callback = function(Value)
			OpenLooping = Value
			if SelectedEgg and Value then
				while OpenLooping and task.wait(1) do
                game:GetService("ReplicatedStorage").Remotes.Egg:FireServer((SelectedEgg),"HatchBtn")
				end
			end
		end
	})
	Main:AddToggle({
		Name = "Tripple Open Egg",
		Default = false,
		Save = true,
		Flag = "SelectedTEgg",
