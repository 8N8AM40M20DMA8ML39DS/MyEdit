		end
	})
	Misc:AddSlider({
	Name = "WalkSpeed",
	Min = 0,
	Max = 50,
	Default = 16,
	Color = Color3.fromRGB(199,40,40),
	Increment = 1,
	ValueName = "Length",
	Callback = function(Value)
	    WalkSpeed = Value
			while WalkSpeed and task.wait() do
	        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = WalkSpeed
		end
