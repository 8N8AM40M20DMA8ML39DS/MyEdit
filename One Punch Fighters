local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("One Punch Fighters", "DarkTheme")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("AutoFarm")
Section:NewToggle("AutoPunch",  "AutoPunch", function(state)
    if state then
        getgenv().Acctack = true
while Acctack do
wait(0.1) -- if your ping is dying put some delay here
local args = {
    [1] = "Normal"
}

game:GetService("ReplicatedStorage").Game.__Remotes.ClickEvent:FireServer(unpack(args))
end
    else
        getgenv().Acctack = tru
while Acctack do
wait(0.1) -- if your ping is dying put some delay here
local args = {
    [1] = "Normal"
}

game:GetService("ReplicatedStorage").Game.__Remotes.ClickEvent:FireServer(unpack(args))
end
    end
end)
